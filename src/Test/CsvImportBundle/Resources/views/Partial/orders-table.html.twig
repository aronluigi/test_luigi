<div class="orders-table">
    <table id="ordersTable" data-js="orderTable">
        <thead>
            <tr>
                <th>#</th>
                <th>Order Id</th>
                <th>Order Nr</th>
                <th>Company</th>
                <th>Street1</th>
                <th>Street2</th>
                <th>Zip</th>
                <th>City</th>
                <th>Country</th>
                <th>Qty</th>
                <th>Price</th>
                <th>Sku</th>
                <th>Product Name</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Email</th>
                <th>Phone Nr</th>
            </tr>
        </thead>
        <tbody>
        {% for order in orders %}
                <tr>
                    <td>{{ order.id }}</td>
                    <td>{{ order.orderId }}</td>
                    <td>{{ order.orderNr }}</td>
                    <td>{{ order.company }}</td>
                    <td>{{ order.streetA }}</td>
                    <td>{{ order.streetB }}</td>
                    <td>{{ order.zip }}</td>
                    <td>{{ order.city }}</td>
                    <td>{{ order.country }}</td>
                    <td>{{ order.qty }}</td>
                    <td>{{ order.price }}</td>
                    <td>{{ order.sku }}</td>
                    <td>{{ order.productName }}</td>
                    <td>{{ order.firstName }}</td>
                    <td>{{ order.lastName }}</td>
                    <td>{{ order.email }}</td>
                    <td>{{ order.phone }}</td>
                </tr>
        {% endfor %}
        </tbody>
    </table>
</div>